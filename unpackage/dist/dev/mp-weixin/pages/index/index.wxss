@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_2380716_6mfvtt1gbdo.eot?t=1615192330500'); /* IE9 */
  src: url('//at.alicdn.com/t/font_2380716_6mfvtt1gbdo.eot?t=1615192330500#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_2380716_6mfvtt1gbdo.woff?t=1615192330500') format('woff'),
  url('//at.alicdn.com/t/font_2380716_6mfvtt1gbdo.ttf?t=1615192330500') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_2380716_6mfvtt1gbdo.svg?t=1615192330500#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
.icon-diqiu:before {
  content: "\e631";
}
.icon-aixin:before {
  content: "\e64a";
}
